<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="370.0" prefWidth="1150.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.sithumonline.view.handler.UsersSearchHandler">
   <children>
      <GridPane layoutX="10.0" layoutY="10.0" prefHeight="90.0" prefWidth="400.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="txtQueryId" />
            <TextField fx:id="txtQueryName" GridPane.columnIndex="1" />
            <Button fx:id="butDelete" mnemonicParsing="false" onAction="#pressDelete" prefHeight="26.0" prefWidth="204.0" text="Delete" GridPane.rowIndex="1" />
            <Button fx:id="butUpdate" mnemonicParsing="false" onAction="#pressUpdate" prefHeight="26.0" prefWidth="193.0" text="Update" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="butSave" mnemonicParsing="false" onAction="#pressSave" prefHeight="26.0" prefWidth="191.0" text="Save" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Button fx:id="butGoMainPlane" mnemonicParsing="false" onAction="#goMainPlane" prefHeight="26.0" prefWidth="188.0" text="Main Plane" GridPane.columnIndex="2" />
         </children>
      </GridPane>
      <ComboBox fx:id="comQueryName" layoutX="7.0" layoutY="134.0" prefHeight="26.0" prefWidth="400.0" />
      <TextField fx:id="txtLogic" layoutX="7.0" layoutY="174.0" prefHeight="26.0" prefWidth="400.0" />
      <TextField fx:id="txtQueryLogic" layoutX="7.0" layoutY="100.0" prefHeight="26.0" prefWidth="400.0" />
      <Label fx:id="labInfo" layoutX="14.0" layoutY="206.0" prefHeight="16.0" prefWidth="285.0" text="Null" />
      <Button fx:id="butApply" layoutX="350.0" layoutY="201.0" mnemonicParsing="false" onAction="#pressApply" text="Apply" />
       <TableView fx:id="tabUser" layoutX="420.0" layoutY="10.0" prefHeight="200.0" prefWidth="528.0" GridPane.columnIndex="3">
           <columns>
               <TableColumn fx:id="tabUserId" prefWidth="75.0" text="user_id" />
               <TableColumn fx:id="tabUsername" prefWidth="90.0" text="username" />
               <TableColumn fx:id="tabPassword" prefWidth="87.0" text="password" />
               <TableColumn fx:id="tabFullname" minWidth="0.0" prefWidth="106.0" text="fullname" />
               <TableColumn fx:id="tabEmail" prefWidth="167.0" text="email" />
           </columns>
       </TableView>
   </children>
</AnchorPane>
